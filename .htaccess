AddType image/svg+xml svg
AddType image/svg+xml svgz

<IfModule mod_rewrite.c>

    Options +FollowSymlinks

    RewriteEngine On

    # deal with admin first

    RewriteCond %{REQUEST_URI} ^/(admin)

    RewriteRule ^admin/assets/(.*)$ backend/web/assets/$1 [L]

    RewriteRule ^admin/css/(.*)$ backend/web/css/$1 [L]

    RewriteRule ^admin/js/(.*)$ frontend/web/js/$1 [L]



    RewriteCond %{REQUEST_URI} !^/backend/web/(assets|css|js)/

    RewriteCond %{REQUEST_URI} ^/(admin)

    RewriteRule ^.*$ backend/web/index.php [L]



    RewriteCond %{REQUEST_URI} ^/(assets|css|js|images)

    RewriteRule ^assets/(.*)$ frontend/web/assets/$1 [L]

    RewriteRule ^css/(.*)$ frontend/web/css/$1 [L]

    RewriteRule ^js/(.*)$ frontend/web/js/$1 [L]

    RewriteRule ^images/(.*)$ frontend/web/images/$1 [L]


    RewriteCond %{REQUEST_URI} ^/attachments

    RewriteRule ^attachments/(.*)$ frontend/web/images/store/$1 [L]


    RewriteCond %{REQUEST_URI} !^/(frontend|backend)/web/(assets|css|js|images)/

    RewriteCond %{REQUEST_URI} !index.php

    RewriteCond %{REQUEST_FILENAME} !-d [OR]

    RewriteCond %{REQUEST_FILENAME} !-f

    RewriteRule ^.*$ frontend/web/index.php

</IfModule>